CREATE TABLE "assets" (id INTEGER PRIMARY KEY AUTOINCREMENT,    original_name TEXT,    path TEXT,    thumb TEXT,    size INTEGER,    width INTEGER,    height INTEGER,    mime TEXT,    orig_dimension TEXT,    active INTEGER,    duration INTEGER,  type TEXT,  time_from INTEGER,  time_to INTEGER,  created_user INTEGER,  created_time INTEGER,  metadata TEXT,  filename TEXT,  label TEXT,  description TEXT)